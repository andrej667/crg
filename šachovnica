#include <iostream>
using namespace std;

int main() {

    string  sachovnica[5][5];
    string znaky[5] = { "A" "B" "C" "D" "E" };

    for (int i = 0; i < 5; i++) {
    
        cout << znaky[i] << ": ";
        for (int j = 0; j < 5; j++) {
        cout << sachovnica[i][j] << "x";


        
cout << endl;
}
    

return 0;
}
